<Window x:Class="SiRat.Popup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SiRat"
        xmlns:vm="clr-namespace:SiRat.ViewModel"
        xmlns:model="clr-namespace:SiRat.Model"
        xmlns:datamodel="clr-namespace:SiRat.Model.Data"
        mc:Ignorable="d"
        Title="Info" Height="300" Width="500">
    <DockPanel>
        <StackPanel x:Name="ButtonList" DockPanel.Dock="Bottom" Margin="16" Orientation="Horizontal" HorizontalAlignment="Right">
    
        </StackPanel>
        <DockPanel x:Name="ContentContainer"  DockPanel.Dock="Top">
            <TextBlock x:Name="ContentText" DockPanel.Dock="Top" Margin="16" TextWrapping="Wrap"/>
        </DockPanel>
    </DockPanel>
</Window>
